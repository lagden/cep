{
  "name": "@tadashi/cep",
  "version": "1.9.1",
  "description": "Busca por CEP diretamente da API dos Correios",
  "keywords": [
    "cep",
    "busca",
    "consulta",
    "correios",
    "lib"
  ],
  "bug": {
    "url": "https://github.com/lagden/cep/issues",
    "email": "lagden@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Thiago Lagden",
    "email": "lagden@gmail.com"
  },
  "repository": "lagden/cep",
  "scripts": {
    "pretest": "xo",
    "test": "ava --serial -t",
    "precoverage": "npm run pretest",
    "coverage": "nyc --reporter=text-summary --reporter=lcov ava --serial -t",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "src/index.js",
  "files": [
    "src",
    "license",
    "readme.md"
  ],
  "dependencies": {
    "soap": "0.33.0"
  },
  "devDependencies": {
    "ava": "3.13.0",
    "nyc": "15.1.0",
    "snyk": "1.414.0",
    "xo": "0.33.1"
  },
  "snyk": true,
  "xo": {
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "padding-line-between-statements": 0,
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "capitalized-comments": 0
    }
  },
  "ava": {
    "timeout": "30s"
  }
}
